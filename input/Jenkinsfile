pipeline {
    agent any
    parameters {
        string(defaultValue:"Hello World!", description: "Greetings", name: "greet")
        booleanParam(defaultValue:false, description: "Enable service?", name: "myBool")
    }

    stages {
        stage ("demo"){
            steps{
                echo("Hello ${greet}")
                echo "Param for boolean variable is set to : ${myBool}"
            }
        }
    }
}